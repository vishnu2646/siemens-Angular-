                { title: "Device type", data: { _: "DeviceTypeName", filter: "DeviceTypeId" }, width: "12%" },
                { title: "Video codec", data: "VideoCodec", width: "15%" },
                { title: "Channels", data: "Channels", width: "4%" },
                { title: "Audio", data: "Audio", width: "9%" },
                { title: "I/O ports", data: "IOPorts", width: "7%" },
                { title: "PTZ support", data: "PTZSupport", width: "9%" },
                { title: "Licenses", data: "Licenses", width: "4%" }
            ],
            dom: "tlp",
            responsive: false,
            autoWidth: false,
            ordering: true,
            rowGroup: { dataSrc: "ManufacturerName" },
            orderFixed: { pre: [0, "asc"] },
            createdRow: createdRowHandler,
            lengthMenu: [[10, 20, 50, 100, -1], [10, 20, 50, 100, "All"]],
            pageLength: 100
        };

        var widgetInstance = new SearchAndFilter.SearchAndFilterWidget("#supportedHwGrid", model, ".results", tableOptions, dataSource);
        SearchAndFilter.WidgetExtensions.FilterStateInUrl(widgetInstance, URI);
        widgetInstance.init();
    });
</script>
