                            .map(function (val) { return featuresHash[val] });
                    } else {
                        features = [];
                    }
                    val.StringSearchData =
                        [val.ArcusVersionName,
                        val.DevicePackName,
                        val.DriverName,
                        val.DeviceName = (val.DriverSdk) ? val.DeviceName + '<br>' + '3rd Party Driver' : val.DeviceName,
                        val.DeviceTypeName,
                        val.FirmwareName,
                        val.ManufacturerName]
                        .concat(features)
                        .join(",");
                    return val;
                });
                return extendedData;
            });

        function createdRowHandler(row, data, index) {
            // Attach a click handler to open the single device page
            var platform = 'XPCO',
                $row = $(row),
                url;

            url = '/community/business-partner-tools/supported-devices/supported-device/' + '?deviceId=' + data.DeviceId + '&platform=' + platform + '&backCloses=true';
            $row.click(function () {
                window.open(url, "_blank");
            });

            // Add highlight on hover
            $row.on("mouseenter", function () {
                $row.addClass("highlighted");
            });
            $row.on("mouseleave", function () {
                $row.removeClass("highlighted");
            })
        }

        var tableOptions =
        {
            data: null,
            ordering: false,
            info: false,
            columns: [
                { data: "ManufacturerName", visible: false, orderable: false },
                { data: "ManufacturerId", visible: false },
